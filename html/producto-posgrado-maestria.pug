   script(src="https://code.jquery.com/jquery-3.6.1.min.js") 
   script(src="../js/global.js")
   script(src="../vendor/slick.min.js")
   script.

      // Accordeon for certificados
      accordeonSection( '._c-certifications-accordeons' )   
      accordeonSection( '._c-copy-and-picture-color-box' )

      // Carousel for modalidades
      $('._c-slide-slick-imgside ._c-slide-slick-imgside--modalidadesList').slick({
         dots: false,
         infinite: false,
         speed: 300,
         slidesToShow: 2,
         centerMode: true,
         variableWidth: true,
         prevArrow: '.modalidadNav.prev',
         nextArrow: '.modalidadNav.next',
         responsive: [
            {
               breakpoint: 700,
               settings: {
                  slidesToShow: 1,
                  slidesToScroll: 1,
                  centerMode: false,
                  variableWidth: false,                     
               }
            }
         ]            
      });

      // Carousel testimonios
      $('._c-testimonios-section--carousel').slick({
         dots: true,
         infinite: false,
         speed: 300,
         slidesToShow: 1,
      });


      // Carousel related careers
      $('#related-careers-list-id').slick({
         dots: false,
         infinite: false,
         speed: 300,
         slidesToShow: 4,
         slidesToScroll: 1,
         responsive: [
            {
               breakpoint: 1080,
               settings: {
                  centerMode: false,
                  slidesToShow: 3,
                  dots: true
               }
            },
            {
               breakpoint: 700,
               settings: {
                  centerMode: false,
                  slidesToShow: 2,
                  dots: true
               }
            },
            {
               breakpoint: 560,
               settings: {
                  centerMode: false,
                  slidesToShow: 1,
                  dots: true
               }
            }
         ]
      });